{
  "containerDefinitions": [
    {
      "name": "gomgom",
      "cpu": 0,
      "portMappings": [
        {
          "name": "gomgom-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "80"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "COOKIE_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:988606121037:secret:gomgom_secret-WBYID5:COOKIE_SECRET::"
        },
        {
          "name": "MONGO_URI",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:988606121037:secret:gomgom_secret-WBYID5:MONGO_URI::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "family": "gomgom_task_definition",
  "executionRoleArn": "arn:aws:iam::988606121037:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
